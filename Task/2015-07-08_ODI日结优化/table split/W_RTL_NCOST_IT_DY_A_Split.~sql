--W_RTL_BCOST_IT_DY_A
--W_RTL_BCOST_IT_DY_A_HIST
--W_RTL_BCOST_IT_DY_A_V

--1.
CREATE TABLE RADM.W_RTL_BCOST_IT_DY_A_HIST AS
SELECT * FROM RADM.W_RTL_BCOST_IT_DY_A WHERE 1=2;

--2.
CREATE OR REPLACE VIEW RADM.W_RTL_BCOST_IT_DY_A_V AS
SELECT * FROM RADM.W_RTL_BCOST_IT_DY_A_HIST
UNION ALL
SELECT * FROM RADM.W_RTL_BCOST_IT_DY_A;




select /*+parallel(32)*/ count(1) from W_RTL_BCOST_IT_DY_A t;
select /*+parallel(32)*/ count(1) from w_rtl_sls_it_dy_a;
