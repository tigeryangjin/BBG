SELECT D.DEPT, D.DEPT_NAME, C.CLASS, C.CLASS_NAME, S.SUBCLASS, S.SUB_NAME
  FROM DEPS D, CLASS C, SUBCLASS S
 WHERE D.DEPT = C.DEPT
   AND D.DEPT = S.DEPT
   AND C.CLASS = S.CLASS
   AND S.SUB_NAME LIKE '%Ó¤%'
 ORDER BY D.DEPT, C.CLASS, S.SUBCLASS;

SELECT * FROM DEPS;

SELECT * FROM CLASS;
SELECT * FROM SUBCLASS;
SELECT * FROM ALL_ALL_TABLES T WHERE T.table_name LIKE '%CLASS%';

SELECT * FROM SUBCLASS T WHERE T.SUB_NAME LIKE '%Ó¤%';
SELECT * FROM SUBCLASS T WHERE T.SUB_NAME LIKE '%Ó×%';
SELECT * FROM SUBCLASS T WHERE T.SUB_NAME LIKE '%ÄÌ·Û%';
SELECT * FROM SUBCLASS T WHERE T.SUB_NAME LIKE '%Äò%';
