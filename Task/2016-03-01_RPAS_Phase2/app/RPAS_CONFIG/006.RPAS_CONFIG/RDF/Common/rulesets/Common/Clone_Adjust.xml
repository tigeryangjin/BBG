<?xml version="1.0" encoding="UTF-8"?>
<data_set type="com.retek.ride.rule.newmodel.RuleGroup">
  <attribute name="name">
    <value>Clone_Adjust</value>
  </attribute>
  <attribute name="description">
    <value>a rule group</value>
  </attribute>
  <attribute name="role">
    <value>user</value>
  </attribute>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>clone_adj_mask</value>
    </attribute>
    <attribute name="description">
      <value>set mask on clone_adj</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>clnAdjMask = !mskClnAdjExcl &amp;&amp; mskEligProdLoc</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>clone_adj_elig_prod_loc</value>
    </attribute>
    <attribute name="description">
      <value>Eligible prod/locs for Clone Adj</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>mskEligProdLoc = if(clnadj &gt; 0 &amp;&amp; tssum(pos, index([CLND].[week], now) - clnslsprdXLXB, index([CLND].[week], now)) &gt; 0 , true, false)</value>
      </attribute>
    </data_model>
  </data_model>
</data_set>
