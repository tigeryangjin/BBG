<?xml version="1.0" encoding="UTF-8"?>
<data_set type="com.retek.ride.rule.newmodel.RuleGroup">
  <attribute name="name">
    <value>AC_IPIStr_1</value>
  </attribute>
  <attribute name="description">
    <value>a rule group</value>
  </attribute>
  <attribute name="role">
    <value>user</value>
  </attribute>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>aswpblndmtrcv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASWPBlndMtrcV = (ACWPSlsUWgtV * ACWPSlsAvgAgU/ACWPSlsAvgAgU.level([prod].top)) + (ACWPSlsRWgtV * ACWPSlsAvgAgR/ACWPSlsAvgAgR.level([prod].top)) + (ACWPGPWgtV * ACWPGPAvgAgR/ACWPGPAvgAgR.level([prod].top)) + (ACWPGPpWgtV * (ACWPGPAvgAgR/ACWPSlsAvgAgR)/(ACWPGPAvgAgR.level([prod].top)/ACWPSlsAvgAgR.level([prod].top))) + (ACWPLoyWgtV * ACDVLoyaltyX/ACDVLoyaltyX.level([prod].top).average_pop) + (ACWPMktBWgtV * ACDVMktBasketX/ACDVMktBasketX.level([prod].top).average_pop)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpattrvalb_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPAttrValB = if(position([attr].[atv] ) == ADWPAttrValTx, true, false )</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>ashdblndmtrcv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASHdBlndMtrcV = if( position([faah].[far] ) == "fa1", ASWPBlndMtrcV, 0)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>aswpattrblndmtrcv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASWPAttrBlndMtrcV = if(ACWPAttrValB &amp;&amp; ACWPAttrNWgtV &gt; 0 , ASHdBlndMtrcV, ignore )</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>aswpattrblndavgv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASWPAttrBlndAvgV = ASWPAttrBlndMtrcV/ASWPAttrBlndMtrcV.level([prod].top )</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>aswpattrvalblndv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASWPAttrValBlndV = ACWPAttrVWgtV * ASWPAttrBlndAvgV</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>aswpattrnameblndv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASWPAttrNameBlndV = ACWPAttrNWgtV * ASWPAttrValBlndV.level([ATTR].[ATN] ).total</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>aswpattripiv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASWPAttrIPIV = ASWPAttrNameBlndV.level([ATTR].top ).total</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>aswpfocarscov_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASWPFocArScoV = if(position([faah].[far] ) == "fa1", ASWPAttrIPIV * ACWPFocArWgtV, ASWPBlndMtrcV * ACWPFocArWgtV )</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>aswpconsegscov_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASWPConSegScoV = ASWPFocArScoV * ACWPConSegWgtV</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>aswpipiscov_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASWPIPIScoV = ASWPConSegScoV</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>ashdipiscov_calc</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASHdIPIScoV = if (ASDVEligibilityB == false, - (ACHdIPISKUX + 1)/100000, if (ASDVMandB, ASWPIPIScoV + ASWPIPIScoV.level([prod].[clss]).max_pop + (1/10000), ASWPIPIScoV))</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>ash1ipiscov_calc</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASH1IPIScoV = (ASHdIPIScoV  * 1000000) + ((ACHdIPISKUX.level([prod].[clss]).max - ACHdIPISKUX + 1)/1000000)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>ashdipiscox_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASHdIPIScoX &lt;- ranksort(ASH1IPIScoV, "descending", [prod].[clss],[loc].[str])</value>
      </attribute>
    </data_model>
  </data_model>
</data_set>
