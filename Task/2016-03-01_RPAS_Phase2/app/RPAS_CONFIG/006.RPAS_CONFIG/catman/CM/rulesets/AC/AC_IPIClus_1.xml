<?xml version="1.0" encoding="UTF-8"?>
<data_set type="com.retek.ride.rule.newmodel.RuleGroup">
  <attribute name="name">
    <value>AC_IPIClus_1</value>
  </attribute>
  <attribute name="description">
    <value>a rule group</value>
  </attribute>
  <attribute name="role">
    <value>user</value>
  </attribute>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpblndmtrcclusv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPBlndMtrcV = (ACWPSlsUWgtV * ACWPSlsAvgAgU/ACWPSlsAvgAgU.level([prod].top)) + (ACWPSlsRWgtV * ACWPSlsAvgAgR/ACWPSlsAvgAgR.level([prod].top)) + (ACWPGPWgtV * ACWPGPAvgAgR/ACWPGPAvgAgR.level([prod].top)) + (ACWPGPpWgtV * (ACWPGPAvgAgR/ACWPSlsAvgAgR)/(ACWPGPAvgAgR.level([prod].top)/ACWPSlsAvgAgR.level([prod].top))) + (ACWPLoyWgtV * ACDVLoyaltyX/ACDVLoyaltyX.level([prod].top).average_pop) + (ACWPMktBWgtV * ACDVMktBasketX/ACDVMktBasketX.level([prod].top).average_pop)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpattrvalb_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPAttrValB = if(position([attr].[atv] ) == ADWPAttrValTx, true, false )</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>achdblndmtrcv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACHdBlndMtrcV = if( position([faah].[far] ) == "fa1", ACWPBlndMtrcV, 0)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpattrblndmtrcv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPAttrBlndMtrcV = if(ACWPAttrValB &amp;&amp; ACWPAttrNWgtV &gt; 0, ACHdBlndMtrcV, ignore )</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpattrblndavgv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPAttrBlndAvgV = ACWPAttrBlndMtrcV/ACWPAttrBlndMtrcV.level([prod].top )</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpattrvalblndv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPAttrValBlndV = ACWPAttrVWgtV * ACWPAttrBlndAvgV</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpattrnameblndv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPAttrNameBlndV = ACWPAttrNWgtV * ACWPAttrValBlndV.level([ATTR].[ATN] ).total</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpattripiv_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPAttrIPIV = ACWPAttrNameBlndV.level([ATTR].top).total</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpfocarscov_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPFocArScoV = if(position([faah].[far] ) == "fa1", ACWPAttrIPIV * ACWPFocArWgtV, ACWPBlndMtrcV * ACWPFocArWgtV )</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpconsegscov_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPConSegScoV = ACWPFocArScoV * ACWPConSegWgtV</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>acwpipiscov_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACWPIPIScoV = ACWPConSegScoV</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>achdipiscov_calc</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACHdIPIScoV = if (ACDVEligibilityB == false, - (ACHdIPISKUX + 1)/100000, if (ACDVMandB, ACWPIPIScoV + ACWPIPIScoV.level([prod].[clss]).max_pop + (1/10000), ACWPIPIScoV))</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>ach1ipiscov_calc</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACH1IPIScoV = (ACHdIPIScoV  * 1000000) + ((ACHdIPISKUX.level([prod].[clss]).max - ACHdIPISKUX)/1000000)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>achdipiscox_ipi</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ACHdIPIScoX &lt;- ranksort(ACH1IPIScoV, "descending", [prod].[clss],[loc].[strc])</value>
      </attribute>
    </data_model>
  </data_model>
</data_set>
