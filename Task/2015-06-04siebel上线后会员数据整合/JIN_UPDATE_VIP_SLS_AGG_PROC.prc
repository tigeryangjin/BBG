            FROM RADM.W_RTL_SLS_TRX_IT_LC_DY_F_BAK B,
                 RABATCHER.W_PRODUCT_D_RTL_TMP     P
           WHERE B.PROD_SCD1_WID = P.PROD_SCD1_WID
