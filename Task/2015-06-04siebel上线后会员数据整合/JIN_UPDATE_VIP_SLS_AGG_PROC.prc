           SUM(B.VIP_RET_TAX_AMT_LCL / B.GLOBAL1_EXCHANGE_RATE) VIP_RET_TAX_AMT_GLOBAL1,
           SUM(B.VIP_RET_TAX_AMT_LCL / B.GLOBAL2_EXCHANGE_RATE) VIP_RET_TAX_AMT_GLOBAL2,
