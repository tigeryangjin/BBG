 WHERE A.DT_WID BETWEEN &BDTWID AND &EDTWID;
 
 --存储过程
JIN_UPDATE_VIP_SLS_TRX_PROC;
	 
--检查更新日期
SELECT /*+PARALLEL(A,20)*/
DISTINCT A.DT_WID
  FROM RADM.W_RTL_SLS_TRX_IT_LC_DY_F A
 WHERE A.VIP_SLS_QTY IS NULL
   AND A.DT_WID BETWEEN 120140101000 AND 120150623000
 ORDER BY A.DT_WID;

--3.历史金立家电会员卡写入W_RTL_SLS_TRX_IT_LC_DY_F
--3.1.CREATE TABLE
CREATE TABLE RADM.BBG_RA_YJ_JL_TRX_HYK_TMP AS
SELECT DISTINCT SLS_TRX_ID, HYK_NO
  FROM BBG_RA_SLS_TRX_JL_V@RA_JL
 WHERE HYK_NO IS NOT NULL;
--3.2.CREATE INDEX
SELECT T.SLS_TRX_ID, COUNT(*)
  FROM RADM.BBG_RA_YJ_JL_TRX_HYK_TMP T
 GROUP BY T.SLS_TRX_ID
HAVING COUNT(*) > 1;
--3.3.UPDATE
UPDATE /*+PARALLEL(A,16)*/ RADM.W_RTL_SLS_TRX_IT_LC_DY_F /*PARTITION(RTL_SLS_TRX_IT_LC_DY_F_201401)*/ A
   SET A.HYK_NO =
       (SELECT /*+PARALLEL(B,16)*/
         B.HYK_NO
          FROM RADM.BBG_RA_YJ_JL_TRX_HYK_TMP B
         WHERE A.SLS_TRX_ID = B.SLS_TRX_ID)
 WHERE EXISTS (SELECT /*+PARALLEL(B,16)*/
         1
          FROM RADM.BBG_RA_YJ_JL_TRX_HYK_TMP B
         WHERE A.SLS_TRX_ID = B.SLS_TRX_ID)
   AND A.HYK_NO IS NULL
   /*AND A.DT_WID = 120140101000*/;
--3.4.CHECK
SELECT *
  FROM RADM.W_RTL_SLS_TRX_IT_LC_DY_F T
 WHERE T.DT_WID = 120150803000
   AND T.BBG_SERVICE_SATISFACTION = 3.5
   AND T.HYK_NO IS NOT NULL;
--3.5.UPDATE会员列数据
  UPDATE /*+PARALLEL(A,16)*/ RADM.W_RTL_SLS_TRX_IT_LC_DY_F A
     SET A.VIP_SLS_QTY                 = A.SLS_QTY,
         A.VIP_SLS_AMT_LCL             = A.SLS_AMT_LCL,
         A.VIP_SLS_PROFIT_AMT_LCL      = A.SLS_PROFIT_AMT_LCL,
         A.VIP_SLS_MANUAL_MKDN_AMT_LCL = A.SLS_MANUAL_MKDN_AMT_LCL,
         A.VIP_SLS_MANUAL_MKUP_AMT_LCL = A.SLS_MANUAL_MKUP_AMT_LCL,
         A.VIP_SLS_TAX_AMT_LCL         = A.SLS_TAX_AMT_LCL,
         A.VIP_SLS_EMP_DISC_AMT_LCL    = A.SLS_EMP_DISC_AMT_LCL,
         A.VIP_SLS_MANUAL_COUNT        = A.SLS_MANUAL_COUNT,
         A.VIP_SLS_SCAN_COUNT          = A.SLS_SCAN_COUNT,
         A.VIP_RET_QTY                 = A.RET_QTY,
         A.VIP_RET_AMT_LCL             = A.RET_AMT_LCL,
         A.VIP_RET_PROFIT_AMT_LCL      = A.RET_PROFIT_AMT_LCL,
         A.VIP_RET_MANUAL_MKDN_AMT_LCL = A.RET_MANUAL_MKDN_AMT_LCL,
         A.VIP_RET_MANUAL_MKUP_AMT_LCL = A.RET_MANUAL_MKUP_AMT_LCL,
         A.VIP_RET_TAX_AMT_LCL         = A.RET_TAX_AMT_LCL,
         A.VIP_RET_EMP_DISC_AMT_LCL    = A.RET_EMP_DISC_AMT_LCL,
         A.VIP_RET_MANUAL_COUNT        = A.RET_MANUAL_COUNT,
         A.VIP_RET_SCAN_COUNT          = A.RET_SCAN_COUNT
   WHERE A.BBG_SERVICE_SATISFACTION = 3.5
     AND A.HYK_NO IS NOT NULL;
  COMMIT;

