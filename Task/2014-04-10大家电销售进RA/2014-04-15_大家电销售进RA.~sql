SELECT * FROM RADM.W_PRODUCT_DS;
SELECT * FROM RADM.W_PRODUCT_ATTR_DS;
SELECT * FROM RADM.W_PRODUCT_DS_TL;
SELECT * FROM RADM.W_PRODUCT_TYPE_D;
SELECT * FROM ALL_ALL_TABLES T WHERE T.table_name = 'W_PRODUCT_TYPE_D';

CREATE TABLE RA_RMS.W_RTL_ITEM_D_TMP_BAK AS
SELECT * FROM RA_RMS.W_RTL_ITEM_D_TMP;


SELECT * FROM RADM.W_PRODUCT_ATTR_D T WHERE T.PRODUCT_ATTR11_NAME = 1;


