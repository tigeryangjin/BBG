--PROD
SELECT T.ROW_WID,
       T.PROD_NUM,
       T.SCD1_WID,
       T.PROD_CAT5_WID,
       T.PROD_CAT5,
       T.PROD_CAT5_WID_AS_WAS,
       T.PROD_CAT5_AS_WAS,
       T.EFFECTIVE_FROM_DT,
       T.EFFECTIVE_TO_DT
  FROM RADM.W_PRODUCT_D T
 WHERE T.PROD_NUM = '100375571';
--SLS
SELECT /*+PARALLEL(32)*/ * FROM RADM.W_RTL_SLS_TRX_IT_LC_DY_F T WHERE T.PROD_SCD1_WID=697932;
