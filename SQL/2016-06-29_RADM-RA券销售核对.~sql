SELECT T.ORG_NUM,
       SUM(((T.SLS_AMT_LCL - T.RET_AMT_LCL) -
           (T.SLS_TAX_AMT_LCL - T.RET_TAX_AMT_LCL)) -
           (T.SLS_PROFIT_AMT_LCL - T.SLS_PROFIT_AMT_LCL)) COST,
       SUM(T.SLS_AMT_LCL - T.RET_AMT_LCL) 含券销售金额,
       SUM((T.SLS_AMT_LCL - T.RET_AMT_LCL) -
           (T.SLS_COUPON_AMT_LCL - T.RET_COUPON_AMT_LCL)) 不含券销售金额,
       SUM(T.SLS_COUPON_AMT_LCL - T.RET_COUPON_AMT_LCL) COUPON
  FROM RADM.W_RTL_SLS_TRX_IT_LC_DY_FS T
 where nvl(T.BBG_SERVICE_SATISFACTION, 0) <> 3.5
 GROUP BY T.ORG_NUM
 ORDER BY 1;

SELECT T.PROD_IT_NUM,
       SUM(((T.SLS_AMT_LCL - T.RET_AMT_LCL) -
           (T.SLS_TAX_AMT_LCL - T.RET_TAX_AMT_LCL)) -
           (T.SLS_PROFIT_AMT_LCL - T.SLS_PROFIT_AMT_LCL)) COST,
       SUM(T.SLS_AMT_LCL - T.RET_AMT_LCL) 含券含税销售金额,
       SUM((T.SLS_AMT_LCL - T.RET_AMT_LCL) -
           (T.SLS_COUPON_AMT_LCL - T.RET_COUPON_AMT_LCL)) 不含券含税销售金额,
       SUM(T.SLS_COUPON_AMT_LCL - T.RET_COUPON_AMT_LCL) COUPON
  FROM RADM.W_RTL_SLS_TRX_IT_LC_DY_FS T
 where NVL(T.BBG_SERVICE_SATISFACTION, 0) <> 3.5
   AND T.ORG_NUM = 120001
 GROUP BY T.PROD_IT_NUM
 ORDER BY 1;
