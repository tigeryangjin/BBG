select * from bbg_item_loc_lastmonth;

CREATE TABLE RMS.BBG_ITEM_LOC_LASTMONTH1130 AS
	SELECT S.ITEM,
				 S.ITEM_PARENT,
				 S.LOC,
				 S.LOC_TYPE,
				 T.REF_NO1 STATUS,
				 S.PRIMARY_SUPP,
				 DATE '2013-11-30' VDATE,
				 SYSDATE CREATE_DATETIME,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE1,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE2,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE3,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE4,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE5,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE6,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE7,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE8,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE9,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE10,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE11,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE12,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE13,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE14,
				 CASE ('' AS VARCHAR2(100)) ATTRIBUTE15
	--DATE '2013-11-30'           CURRENT_DATETIME
		FROM RADM.BBG_RA_ITEM_LOC_D@RMS_RA T, RA_RMS.ITEM_LOC_SOH_20131130 S
	 WHERE T.ITEM = S.ITEM
		 AND T.LOC = S.LOC
		 AND T.EFFECTIVE_FROM_DT <= DATE '2013-11-30'
		 AND T.EFFECTIVE_TO_DT >= DATE '2013-11-30';
