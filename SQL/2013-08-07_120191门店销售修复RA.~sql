SELECT SUM(T.SLS_AMT_LCL),SUM(T.SLS_AMT) FROM W_RTL_SLS_IT_DY_A T
WHERE T.DT_WID = '120130805000';

SELECT * FROM RA_RMS.W_RTL_SLS_TRX_TMP_A T WHERE T.DAY_DT = DATE'2013-08-05' AND T.ORG_NUM='120191'

SELECT * FROM RA_RMS.XTERN_RDWT WHERE BUSINESS_DT='20130805' ;
SELECT * FROM RA_RMS.BBG_XTERN_RDWT WHERE BUSINESS_DT='20130805' AND LOCATION='0000120191' ;
SELECT * FROM RA_RMS.BBG_XTERN_RDWT_HIST WHERE BUSINESS_DT='20130805' AND LOCATION='0000120191'  ;

SELECT DISTINCT T.BUSINESS_DT FROM RA_RMS.BBG_XTERN_RDWT T

--8.5 120191销售插入RA_RMS.BBG_XTERN_RDWT表
INSERT INTO RA_RMS.BBG_XTERN_RDWT
SELECT * FROM RA_RMS.BBG_XTERN_RDWT_HIST WHERE BUSINESS_DT='20130805' AND LOCATION='0000120191'

--数据暂时备份到BBG_XTERN_RDWT_HIST_08_05
CREATE TABLE BBG_XTERN_RDWT_HIST_08_05 AS
SELECT * FROM RA_RMS.BBG_XTERN_RDWT_HIST WHERE BUSINESS_DT='20130806';

SELECT COUNT(*) FROM  BBG_XTERN_RDWT_HIST_08_05;

--删除RA_RMS.BBG_XTERN_RDWT_HIST表非8.5的数据.
DELETE RA_RMS.BBG_XTERN_RDWT  WHERE BUSINESS_DT='20130806';

TRUNCATE TABLE ra_rms.C_LOAD_DATES;

SELECT DISTINCT BUSINESS_DT FROM RA_RMS.BBG_XTERN_RDWT;
SELECT DISTINCT BUSINESS_DT FROM RA_RMS.BBG_XTERN_RDWT_HIST;

SELECT COUNT(*) FROM RA_RMS.BBG_XTERN_RDWT WHERE BUSINESS_DT='20130804';
SELECT COUNT(*) FROM RA_RMS.BBG_XTERN_RDWT_HIST WHERE BUSINESS_DT='20130804';

SELECT DISTINCT TRAN_DATE FROM TRAN_DATA;

