
SELECT DY.PER_NAME_WEEK /*, DY.JULIAN_WEEK_NUM*/, COUNT(HY.HYID)
  FROM RADM.HYK_HYXX HY, RADM.W_DAY_D DY
 WHERE TO_DATE(TO_CHAR(HY.JKRQ, 'YYYY-MM-DD'), 'YYYY-MM-DD') =
       TO_DATE(TO_CHAR(DY.DAY_DT, 'YYYY-MM-DD'), 'YYYY-MM-DD')
   AND HY.jkrq BETWEEN date '2012-06-28' and date '2014-03-05'
 GROUP BY DY.PER_NAME_WEEK /*, DY.JULIAN_WEEK_NUM
 ORDER BY DY.JULIAN_WEEK_NUM*/
;

SELECT * FROM RADM.W_PARTY_ORG_D T WHERE T.SUPPLIER_NUM = '33003008';

SELECT *
  FROM RADM.HYK_HYXX t
 where t.jkrq date '2012-06-28'
   and date '2014-03-05';

SELECT T.DAY_DT,
       T.DAY_OF_WEEK,
       T.DAY_OF_WEEK,
       T.ENT_DAY_OF_WEEK,
       T.PER_NAME_WEEK
  FROM RADM.W_DAY_D T
 ORDER BY T.DAY_DT;

SELECT * FROM RADM.W_DAY_D T ORDER BY T.DAY_DT;
