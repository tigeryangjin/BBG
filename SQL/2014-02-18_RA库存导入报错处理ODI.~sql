SELECT *
  FROM ALL_ALL_TABLES T
 WHERE T.owner = 'RADM'
   AND T.table_name LIKE '%INV%';
--BACKUP
CREATE TABLE W_RTL_INV_CL_LC_DY_A_0217 AS
  SELECT * FROM W_RTL_INV_CL_LC_DY_A T WHERE T.DT_WID = '120140217000';
CREATE TABLE W_RTL_INV_DP_LC_DY_A_0217 AS
  SELECT * FROM W_RTL_INV_DP_LC_DY_A T WHERE T.DT_WID = '120140217000';
CREATE TABLE W_RTL_INV_IT_DY_A_0217 AS
  SELECT * FROM W_RTL_INV_IT_DY_A T WHERE T.DT_WID = '120140217000';
CREATE TABLE W_RTL_INV_IT_LC_DY_F_0217 AS
  SELECT *
    FROM W_RTL_INV_IT_LC_DY_F T
   WHERE T.FROM_DT_WID = '120140217000';
CREATE TABLE W_RTL_INV_IT_LC_DY_FS_0217 AS
  SELECT * FROM W_RTL_INV_IT_LC_DY_FS T WHERE T.DAY_DT = DATE'2014-02-17';
CREATE TABLE W_RTL_INV_SC_DY_A_0217 AS
  SELECT * FROM W_RTL_INV_SC_DY_A T WHERE T.DT_WID = '120140217000';
CREATE TABLE W_RTL_INV_SC_LC_DY_A_0217 AS
  SELECT * FROM W_RTL_INV_SC_LC_DY_A T WHERE T.DT_WID = '120140217000';
--DELETE DATA
DELETE W_RTL_INV_CL_LC_DY_A T WHERE T.DT_WID = '120140217000';
DELETE W_RTL_INV_DP_LC_DY_A T WHERE T.DT_WID = '120140217000';
DELETE W_RTL_INV_IT_DY_A T WHERE T.DT_WID = '120140217000';
DELETE W_RTL_INV_IT_LC_DY_F T WHERE T.FROM_DT_WID = '120140217000';
DELETE W_RTL_INV_IT_LC_DY_FS T WHERE T.DAY_DT = DATE'2014-02-17';
DELETE W_RTL_INV_SC_DY_A T WHERE T.DT_WID = '120140217000';
DELETE W_RTL_INV_SC_LC_DY_A T WHERE T.DT_WID = '120140217000';

SELECT COUNT(*)
  FROM W_RTL_INV_IT_LC_DY_F T
 WHERE T.FROM_DT_WID = '120140217000';
SELECT * FROM W_RTL_INV_CL_LC_DY_A T WHERE T.DT_WID = '120140217000';
