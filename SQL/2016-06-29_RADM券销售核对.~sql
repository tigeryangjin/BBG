
SELECT T.BUSINESS_DT,
       T.LOCATION,
       T.TRANSACTION_HEADER_NO,
       T.ITEM_SEQ_NO,
       T.ITEM,
       T.PROMO_TRANSACTION_TYPE,
       T.TOT_SALES_QUANTITY,
       T.TOT_TRANSACTION_VAL,
       T.DISCOUNT_VAL
  FROM RA_RMS.BBG_XTERN_RDWT T
 WHERE T.LOCATION = '0000120001'
   AND T.ITEM = '100440011'
 ORDER BY T.TRANSACTION_HEADER_NO, T.ITEM_SEQ_NO,T.PROMO_TRANSACTION_TYPE;

SELECT T.DAY_DT,
       T.ORG_NUM,
       T.SLS_TRX_ID,
       T.IT_SEQ_NUM,
       T.PROD_IT_NUM,
       T.PROMO_TYPE,
       T.TRX_QTY,
       T.TRX_AMT,
       T.DISC_AMT
  FROM RA_RMS.W_RTL_SLS_TRX_TMP_A T
 WHERE T.ORG_NUM = 120001
   AND T.PROD_IT_NUM = '100440011'
 ORDER BY T.SLS_TRX_ID, T.IT_SEQ_NUM,T.PROMO_TYPE;

SELECT T.DAY_DT,
       T.ORG_NUM,
       T.SLS_TRX_ID,
       T.IT_SEQ_NUM,
       T.PROD_IT_NUM,
       T.PROMO_TYPE,
       T.TRX_QTY,
       T.TRX_AMT,
       T.BBG_COUPON_TRX_AMT
  FROM RA_RMS.W_RTL_SLS_TRX_TMP T
 WHERE T.ORG_NUM = 120001
   AND T.PROD_IT_NUM = '100440011'
 ORDER BY T.SLS_TRX_ID, T.IT_SEQ_NUM,T.PROMO_TYPE;
 
------- 
SELECT *
  FROM RA_RMS.BBG_XTERN_RDWT T
 WHERE T.LOCATION = '0000120001'
   AND T.ITEM = '100440011'
 ORDER BY T.TRANSACTION_HEADER_NO, T.ITEM_SEQ_NO,T.PROMO_TRANSACTION_TYPE;

SELECT *
  FROM RA_RMS.W_RTL_SLS_TRX_TMP_A T
 WHERE T.ORG_NUM = 120001
   AND T.PROD_IT_NUM = '100440011'
 ORDER BY T.SLS_TRX_ID, T.IT_SEQ_NUM,T.PROMO_TYPE;

SELECT *
  FROM RA_RMS.W_RTL_SLS_TRX_TMP T
 WHERE T.ORG_NUM = 120001
   AND T.PROD_IT_NUM = '100440011'
 ORDER BY T.SLS_TRX_ID, T.IT_SEQ_NUM,T.PROMO_TYPE;

