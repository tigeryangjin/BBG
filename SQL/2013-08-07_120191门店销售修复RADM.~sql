SELECT * FROM RA_RMS.BBG_RA_SLS_CHECK_V WHERE DAY_DT=DATE'2013-08-05' ;

SELECT ROUND(SUM(NVL(CASE
                 WHEN T.TRANSACTION_TYPE = 'SALE' THEN
                  T.TOT_TRANSACTION_VAL / 10000
                 ELSE
                  -T.TOT_TRANSACTION_VAL / 10000
               END,
               0)),2) AMT FROM RA_RMS.BBG_XTERN_RDWT_HIST T WHERE T.BUSINESS_DT='20130805';


--含税 14745716.9755
SELECT SUM(T.TOTAL_RETAIL) 
FROM RA_RMS.TRAN_DATA_HISTORY_PACK_V T
 WHERE T.TRAN_CODE IN (2, 3)
   AND T.TRAN_DATE = DATE '2013-08-05';
 
--未税 14539250.8185224  
SELECT SUM(T.TOTAL_RETAIL / (1 + (CASE
             WHEN T.TRAN_CODE = 3 THEN
              T.VAT_RATE / 100
             ELSE
              0
           END))) AMT 
FROM RA_RMS.TRAN_DATA_HISTORY_PACK_V T
 WHERE T.TRAN_CODE IN (2, 3)
   AND T.TRAN_DATE = DATE '2013-08-05';
   
--8.5 120191未传到RA
SELECT * FROM RA_RMS.BBG_XTERN_RDWT_V T WHERE T.BUSINESS_DT='20130805' AND T.LOCATION='0000120191'

SELECT * FROM TRAN_DATA T WHERE T.TRAN_DATE = DATE'2013-08-05' AND T.LOCATION='120191';
SELECT * FROM TRAN_DATA_HISTORY T WHERE T.TRAN_DATE = DATE'2013-08-05' AND T.LOCATION='120191'; 

SELECT * FROM RA_RMS.BBG_XTERN_RDWT_HIST T WHERE T.BUSINESS_DT='20130805' AND T.LOCATION='0000120191';


W_RTL_SLS_TRX_IT_LC_DY_FS

SELECT * FROM W_RTL_SLS_TRX_IT_LC_DY_FS T WHERE T.DAY_DT=DATE'2013-08-05';

SELECT * FROM W_RTL_SLS_TRX_IT_LC_DY_F T WHERE T.DT_WID='120130805000' AND T.ORG_WID=173 ;

DELETE W_RTL_SLS_TRX_IT_LC_DY_F T  WHERE T.DT_WID='120130805000' AND T.ORG_WID=173 ;

SELECT * FROM W_INT_ORG_D WHERE ORG_NUM='120191'

SELECT DISTINCT T.DAY_DT FROM W_RTL_SLS_TRX_IT_LC_DY_FS T ;
SELECT DISTINCT T.DT_WID FROM RABATCHER.W_RTL_SLS_TRX_IT_LC_DY_TMP T ;

SELECT * FROM RABATCHER.W_RTL_SLS_TRX_IT_LC_DY_TMP T WHERE T.DT_WID='120130805000' and t.org_wid=173;

TRUNCATE TABLE C_LOAD_DATES;
