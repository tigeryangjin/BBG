--LOC:120149,DATE:2016-04-20,ITEM:102350811,REF_NO5=88
--LOC:120178,DATE:2016-04-21,ITEM:102350811,REF_NO5=88
--LOC:120189,DATE:2016-04-22,ITEM:102350811,REF_NO5=88
SELECT * FROM SA_TRAN_ITEM T WHERE T.STORE=120149 AND T.DAY=20 AND T.ITEM='102350811' FOR UPDATE;
SELECT * FROM SA_TRAN_ITEM T WHERE T.STORE=120178 AND T.DAY=21 AND T.ITEM='102350811' FOR UPDATE;
SELECT * FROM SA_TRAN_ITEM T WHERE T.STORE=120189 AND T.DAY=22 AND T.ITEM='102350811' FOR UPDATE;

SELECT * FROM RADM.W_RTL_SLS_TRX_IT_LC_DY_FS_BAK FOR UPDATE;
SELECT * FROM RADM.W_RTL_SLS_TRX_IT_LC_DY_FS_0421 FOR UPDATE;
SELECT * FROM RADM.W_RTL_SLS_TRX_IT_LC_DY_FS_0422 FOR UPDATE;

