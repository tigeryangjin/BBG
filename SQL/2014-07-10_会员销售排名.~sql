SELECT H.HYK_NO,
       H.HY_NAME,
       G.SEX,
       G.PHONE,
       COUNT(S.XFJLID) CNT,
       SUM(S.JE) AMT
  FROM RADM.HYXFJL S, RADM.MDDY M, RADM.HYK_HYXX H, RADM.HYK_GRXX G
 WHERE S.MDID = M.MDID
   AND S.HYID = H.HYID
   AND S.HYID = G.HYID
   AND M.SHDM = 'CS'
   AND S.JZRQ BETWEEN DATE '2014-06-01' AND DATE
 '2014-06-30'
   AND SUBSTR(M.MDDM, 3, 6) IN ('120211',
                                '120213',
                                '120194',
                                '120202',
                                '120188',
                                '120183',
                                '120144',
                                '120064',
                                '120051',
                                '120063',
                                '120199',
                                '120177',
                                '120029',
                                '120210',
                                '120182',
                                '120201',
                                '120206',
                                '120205',
                                '120170',
                                '120179',
                                '120020',
                                '120180',
                                '120138',
                                '120153',
                                '120149',
                                '120015',
                                '120173',
                                '120189',
                                '120036',
                                '120024',
                                '120146',
                                '120027',
                                '120129',
                                '120185',
                                '120127',
                                '120134',
                                '120135',
                                '120028',
                                '120159',
                                '120195',
                                '120060',
                                '120033',
                                '120082',
                                '120151',
                                '120164',
                                '120095',
                                '120122',
                                '120065',
                                '120035',
                                '120110',
                                '120056',
                                '120022',
                                '120123',
                                '120191',
                                '120075',
                                '120045',
                                '120186',
                                '120168',
                                '120141',
                                '120053',
                                '120021',
                                '120080',
                                '120156',
                                '120125',
                                '120113',
                                '120169',
                                '120098',
                                '120115',
                                '120087',
                                '120081',
                                '120176',
                                '120014',
                                '120145')
 GROUP BY H.HYK_NO, H.HY_NAME, G.SEX, G.PHONE
 ORDER BY SUM(S.JE) DESC;
 
 SELECT * FROM RADM.HYXFJL T WHERE T.HYID=310032773 AND T.JZRQ BETWEEN DATE '2014-06-01' AND DATE
 '2014-06-30';
