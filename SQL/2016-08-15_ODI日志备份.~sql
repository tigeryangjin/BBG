--
INSERT /*+PARALLEL(16)*/
INTO RADM.SNP_SESSION A
  SELECT /*+PARALLEL(16)*/
   *
    FROM ODI_WREP_USER.SNP_SESSION B
   WHERE NOT EXISTS (SELECT /*+PARALLEL(16)*/
           1
            FROM RADM.SNP_SESSION C
           WHERE B.SESS_NO = C.SESS_NO);
COMMIT;

INSERT /*+PARALLEL(16)*/
INTO RADM.SNP_SESS_STEP A
  SELECT /*+PARALLEL(16)*/
   *
    FROM ODI_WREP_USER.SNP_SESS_STEP B
   WHERE NOT EXISTS (SELECT /*+PARALLEL(16)*/
           1
            FROM RADM.SNP_SESS_STEP C
           WHERE B.SESS_NO = C.SESS_NO
             AND B.NNO = C.NNO);
COMMIT;

INSERT /*+PARALLEL(16)*/
INTO RADM.SNP_SESS_TASK_LOG A
  SELECT /*+PARALLEL(16)*/
   *
    FROM ODI_WREP_USER.SNP_SESS_TASK_LOG B
   WHERE NOT EXISTS (SELECT /*+PARALLEL(16)*/
           1
            FROM RADM.SNP_SESS_TASK_LOG C
           WHERE B.SESS_NO = C.SESS_NO
             AND B.NNO = C.NNO
             AND B.NB_RUN = C.NB_RUN);
COMMIT;

--
CREATE TABLE RADM.SNP_SESSION AS
  SELECT * FROM ODI_WREP_USER.SNP_SESSION;

CREATE TABLE RADM.SNP_SESS_STEP AS
  SELECT * FROM ODI_WREP_USER.SNP_SESS_STEP;

CREATE TABLE RADM.SNP_SESS_TASK_LOG AS
  SELECT * FROM ODI_WREP_USER.SNP_SESS_TASK_LOG;
