--5.

--TMP
SELECT T.LOCATION, TO_NUMBER(T.LOCATION) FROM RA_RMS.BBG_XTERN_RDWT T;
SELECT * FROM W_RTL_SLS_TRX_IT_LC_DY_FS@RMS_RA;
